{
	"name": "asb-export-server",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"start": "node build",
		"docker:build": "docker buildx build --platform=linux/amd64,linux/arm64/v8 -t coldino/asb-export-server --push .",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:unit": "vitest",
		"test:e2e-watch": "PWTEST_WATCH=1 playwright test",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- --run && npm run test:e2e"
	},
	"devDependencies": {
		"@playwright/test": "^1.52.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/enhanced-img": "^0.4.4",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.8",
		"@types/node": "^22.15.29",
		"@typescript-eslint/eslint-plugin": "^8.33.0",
		"@typescript-eslint/parser": "^8.33.0",
		"@unocss/extractor-svelte": "^65.5.0",
		"@unocss/preset-uno": "^65.5.0",
		"@unocss/reset": "^65.5.0",
		"@unocss/transformer-directives": "^65.5.0",
		"eslint": "^9.28.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^2.46.1",
		"jsdom": "^26.1.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.33.12",
		"svelte-check": "^4.2.1",
		"sveltekit-rate-limiter": "^0.6.1",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3",
		"undici": "^7.10.0",
		"unocss": "^65.5.0",
		"vite": "^6.3.5",
		"vitest": "^3.1.4"
	},
	"type": "module",
	"engines": {
		"node": ">=20.18.1 <23.0.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"sharp"
		]
	}
}
